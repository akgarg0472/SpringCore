<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.springcore.beanLifeCycle.xml.Laptop" id="laptop" init-method="init" destroy-method="destroy">
        <property name="brand" value="Apple"/>
        <property name="model" value='Macbook Pro 2020 16"'/>
        <property name="price" value="1275.40"/>
    </bean>
</beans>